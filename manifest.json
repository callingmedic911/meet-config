{
  "manifest_version": 2,
  "name": "Config for Meet",
  "version": "1.3",

  "description": "Set default configuration for Google Meet.",

  "icons": {
    "48": "icons/meet-config-48.png",
    "96": "icons/meet-config-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://meet.google.com/*"],
      "js": ["conductor.js"]
    }
  ]
}
