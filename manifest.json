{
  "manifest_version": 2,
  "name": "Config for Meet",
  "version": "1.0",

  "description": "Set default configuration for Google Meet.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://meet.google.com/*"],
      "js": ["conductor.js"]
    }
  ]
}
